    <section id="blog-grid">
		<div class="container">
			
			<!-- blog menu end -->
			<!-- VIDEO POST #1 -->
		 <% @videos.each do |video| %>
			<article class="video-item col-6">
					<div class="video-hold">
						
						<a href="/rfcvideo/<%= video.id  %>"><%= image_tag("video_fake.jpg", :width =>"560",  :height=>"315") %></a>
					</div>
					<div class="video-title">
						<h3><%= video.name.gsub('/system/videos/videos/', '')  rescue "-" %></h3>
						<p>Highlight: <%= video.desc.gsub('{"desc"=>"', '').gsub('"}', '')  rescue "-" %></p>
					</div>
			</article>
		 <% end %>
			
			
			
			<!-- PAGINATION -->
			<div class="pagination">
						<%=  will_paginate @videos %>
			</div>
		</div>
	</section>



	
